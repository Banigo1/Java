package com.ratelsoft.tutorials;

public class LearnArray {
	
	public static void main (String args []){
	
int [] myArr = {6,42,3,7};

int sum = 2;{
	
for(int x = 0; x<myArr.length; x++){
	sum+=myArr[x];
}
System.out.println("------------------------------------------" +sum);
}

int sample [][]= {{1,2,3},{4,5,6}};

int v = sample [0][0];
System.out.println("v = " + v);
}
}
