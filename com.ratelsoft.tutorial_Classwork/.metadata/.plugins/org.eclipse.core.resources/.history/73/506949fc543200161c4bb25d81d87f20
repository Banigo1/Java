package com.ratelsoft.tutorials;

public class Loops {
	public static void main(String [] args){
		for (int i = 1; i<=2; i++){
			for (int j = 1; j<=3; j++){
				for (int k = 1; k<=4; k++){
					for (int l = 1; l<=5; l++){
				System.out.print(i + j + k + l);
				System.out.print(", " );
			}
			System.out.println();
		}
		
	}  

}
}
}
